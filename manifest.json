{
  "name": "梦想综合搜索",
  "description": "一款简洁的综合搜索扩展，可自定义右键搜索和搜索按钮。",
  "version": "2.0.1",
  "manifest_version": 3,
  "icons": {
	"128": "128.png"
  },
  "background": {
	"service_worker": "background.js"
  },
  "action": {
	"default_popup": "popup.html",
	"default_title": "梦想综合搜索",
	"default_icon": "128.png"
  },
  "options_ui": {
	"page": "option.html",
	"open_in_tab": true
  },
  "permissions": [
	"storage",
	"contextMenus"
  ],
  "host_permissions": [],
  "homepage_url": "https://github.com/ryapp/dream_search",
  "commands": {
	"_execute_action": {
	  "suggested_key": {
		"default": "Ctrl+Shift+S",
		"mac": "MacCtrl+Shift+S"
	  },
	  "description": "打开设置"
	}
  }
}
